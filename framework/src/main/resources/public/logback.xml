<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>[%d{yy-MM-dd HH:mm:ss}] %-5p : %m%n</pattern>
    </encoder>
  </appender>
  <logger name="cn.emagsoftware" level="DEBUG"/>
  <root level="INFO">
    <appender-ref ref="stdout"/>
  </root>
</configuration>